<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.iarosinternational.corsoandroid.app.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_courses"
        android:name="com.iarosinternational.corsoandroid.app.ui.courses.CoursesFragment"
        android:label="@string/title_courses"
        tools:layout="@layout/fragment_courses" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.iarosinternational.corsoandroid.app.ui.contacts.ContactsFragment"
        android:label="@string/title_contacts"
        tools:layout="@layout/fragment_contacts" />

    <fragment
        android:id="@+id/navigation_mission"
        android:name="com.iarosinternational.corsoandroid.app.ui.mission.MissionFragment"
        android:label="@string/title_mission"
        tools:layout="@layout/fragment_mission" />

    <fragment
        android:id="@+id/navigation_privacy"
        android:name="com.iarosinternational.corsoandroid.app.ui.privacy.PrivacyFragment"
        android:label="@string/title_privacy"
        tools:layout="@layout/fragment_privacy" />
</navigation>